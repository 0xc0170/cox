<?xml version="1.0" ?><Component>    <Name>SDCard_SPI_Driver</Name>    <Description>SDCard Driver communication with SPI(based on CoX)</Description>    <Version>2.1.1.0</Version>    <KeyWord>SD, SDCard, SD Card, Card, SDSC, SDHC, SDXC, MMC, Driver, CoX</KeyWord>        <Filter>        <Interface>SPI</Interface>        <CoXBased>true</CoXBased>    </Filter><!-- Categorys example for all components, Layer.Cate, Layer.Cate.SubCate can be empty      <Categorys>        <Layer name="Middleware">            <Cate name="Audio">                <SubCate name="MP3" />            </Cate>        </Layer>    </Categorys> -->    <Categorys>        <Layer name="Driver">            <Cate name="Driver" />        </Layer>    </Categorys>         <SupportDevices>        <Device>SDCard(SDSC, SDHC, SDXC)</Device>    </SupportDevices>        <Files>        <File>$Driver_ROOT$\lib\*</File>    </Files>        <CoIDEFiles>        <File>PROJECT_ROOT$\CoX\CoX_Driver\SDCard\hw_sdcard.h</File>        <File>PROJECT_ROOT$\CoX\CoX_Driver\SDCard\sdcard.h</File>        <File>PROJECT_ROOT$\CoX\CoX_Driver\SDCard\sdcard.c</File>    </CoIDEFiles>        <DependentComponentInterfaceTag>        <Tag>CoX.SPI</Tag>        <Tag>CoX.GPIO</Tag>        <Tag>CoX.SysCtl</Tag>    </DependentComponentInterfaceTag>        <SupportMCUs>        <MCU></MCU>    </SupportMCUs>        <SupportMCUSeries>        <Series>All</Series>    </SupportMCUSeries>        <Doc>$Driver_ROOT$\doc\</Doc>  </Component>