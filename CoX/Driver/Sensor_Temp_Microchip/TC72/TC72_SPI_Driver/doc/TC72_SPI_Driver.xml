<?xml version="1.0" ?><Component>    <Name>TC72_SPI_Driver</Name>    <Description>TC72 Temperature Driver using SPI interface</Description>    <Version>1.0.0</Version>    <KeyWord>TC72, Temperature, Sensor, SPI Driver, COX</KeyWord>        <Filter>        <Interface>SPI</Interface>        <COXBased>true</COXBased>    </Filter><!-- Categorys example for all components, Layer.Cate, Layer.Cate.SubCate can be empty      <Categorys>        <Layer name="Middleware">            <Cate name="Audio">                <SubCate name="MP3" />            </Cate>        </Layer>    </Categorys> -->    <Categorys>        <Layer name="Driver">            <Cate name="Driver" />        </Layer>    </Categorys>         <SupportDevices>        <Device>TC72</Device>    </SupportDevices>        <Files>        <File>$Driver_ROOT$\lib\*</File>    </Files>        <CoIDEFiles>        <File>PROJECT_ROOT$\CoX\TC72_SPI_Driver\hw_tc72.h</File>        <File>PROJECT_ROOT$\CoX\TC72_SPI_Driver\tc72.h</File>        <File>PROJECT_ROOT$\CoX\TC72_SPI_Driver\tc72.c</File>    </CoIDEFiles>        <DependentComponentInterfaceTag>        <Tag>CoX.SPI</Tag>        <Tag>CoX.GPIO</Tag>        <Tag>CoX.SysCtl</Tag>    </DependentComponentInterfaceTag>        <SupportMCUs>        <MCU></MCU>    </SupportMCUs>        <SupportMCUSeries>        <Series>All</Series>    </SupportMCUSeries>        <Doc>$Driver_ROOT$\doc\</Doc>  </Component>