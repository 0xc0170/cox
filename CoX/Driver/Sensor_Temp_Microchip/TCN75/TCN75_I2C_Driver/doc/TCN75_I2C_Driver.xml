<?xml version="1.0" ?><Component>    <Name>TCN75_I2C_Driver</Name>    <Description>TCN75 Temperature Driver using I2C interface</Description>    <Version>1.0.0</Version>    <KeyWord>TCN75, Temperature, Sensor, I2C, Driver, COX</KeyWord>        <Filter>        <Interface>I2C</Interface>        <COXBased>true</COXBased>    </Filter><!-- Categorys example for all components, Layer.Cate, Layer.Cate.SubCate can be empty      <Categorys>        <Layer name="Middleware">            <Cate name="Audio">                <SubCate name="MP3" />            </Cate>        </Layer>    </Categorys> -->    <Categorys>        <Layer name="Driver">            <Cate name="Driver" />        </Layer>    </Categorys>         <SupportDevices>        <Device>TCN75</Device>    </SupportDevices>        <Files>        <File>$Driver_ROOT$\lib\*</File>    </Files>        <CoIDEFiles>        <File>PROJECT_ROOT$\CoX\TCN75_I2C_Driver\xhw_TCN75.h</File>        <File>PROJECT_ROOT$\CoX\TCN75_I2C_Driver\TCN75.h</File>        <File>PROJECT_ROOT$\CoX\TCN75_I2C_Driver\TCN75.c</File>    </CoIDEFiles>        <DependentComponentInterfaceTag>        <Tag>CoX.I2C</Tag>        <Tag>CoX.GPIO</Tag>        <Tag>CoX.SysCtl</Tag>    </DependentComponentInterfaceTag>        <SupportMCUs>        <MCU></MCU>    </SupportMCUs>        <SupportMCUSeries>        <Series>All</Series>    </SupportMCUSeries>        <Doc>$Driver_ROOT$\doc\</Doc>  </Component>